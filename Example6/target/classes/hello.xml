<?xml version="1.0" encoding="UTF-8"?>
<beans 

    xmlns="http://www.springframework.org/schema/beans" 

    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 

    xmlns:p="http://www.springframework.org/schema/p" 

    xsi:schemaLocation="http://www.springframework.org/schema/beans 

               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 

  

<bean id="hello" class="com.pack.HelloWorld"> 
<property name="message1" value="Vimal"></property> 
<property name="message2" value="Kesari"></property> 
</bean> 
<bean id="hello1" class="com.pack.HelloIndia" parent="hello">  
<property name="message3" value=" Bolon Zubban Kesari"></property> 
</bean> 
  

</beans> 
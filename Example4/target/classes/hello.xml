<?xml version="1.0" encoding="UTF-8"?>
<beans 

    xmlns="http://www.springframework.org/schema/beans" 

    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 

    xmlns:p="http://www.springframework.org/schema/p" 

    xsi:schemaLocation="http://www.springframework.org/schema/beans 

               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 

  

<bean id="collectionbean" class="com.pack.JavaCollection"> 

<property name="address_list">
<list>
<value>India</value>
<value>Australia</value>
<value>France</value>
<value>Germany</value>
</list>
</property>

<property name="address_set">
<set>
<value>Orange</value>
<value>Apple</value>
<value>Banana</value>
<value>Strawberry</value>
</set>
</property>

<property name="address_map">
<map>
<entry key="1" value="alu"/>
<entry key="2" value="potol"/>
<entry key="3" value="jhinge"/>
<entry key="4" value="lau"/>
</map>
</property>

<property name="address_prop">
<props>
<prop key="one">Vimal</prop>
<prop key="two">Kesari</prop>
<prop key="three">Bolo</prop>
<prop key="four">Zubaan</prop>
<prop key="five">Kesari</prop>
</props>
</property>

</bean>
</beans> 